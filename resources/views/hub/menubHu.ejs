<div class="product-details">
          <div class="pro-head">
            <h1>
              <span><ion-icon name="today"></ion-icon></span>Product <%=
              menus.length %>
            </h1>
            <a href="/n-com/addpro"
              ><span><ion-icon name="add-circle"></ion-icon></span>add
              product</a
            >
          </div>
          <!-- pro-head end -->
          <div class="pro-list">
            <!-- singel product start -->
            <% menus.forEach((pro)=>{ %>
            <div class="singel-pro">
              <div class="img">
                <img src="<%= pro.img %>" alt="" />
              </div>
              <div class="detais-oi">
                <p><%= pro._id %></p>
                <p><%= pro.name %></p>
                <p><%= pro.price %> tk</p>
                <div class=" <%= pro.active? 'ac-pro' : 'dac-pro' %>">
                  <ion-icon
                    name="<%= pro.active? 'laptop' : 'warning' %>"
                  ></ion-icon
                  ><%= pro.active? "active" : "deactive" %>
                </div>
              </div>
              <div class="pro-option">
                <!-- laptop -->
                <a href="/n-com/pro/status/<%= pro._id %>"
                  ><span
                    ><ion-icon
                      name="<%= pro.active? 'warning' : 'laptop' %>"
                    ></ion-icon></span
                  ><%= pro.active?"deactive" : "active" %></a
                >
                <a href="/n-com/pro/delete/<%= pro._id %>"
                  ><span><ion-icon name="trash"></ion-icon></span>dellet</a
                >
                <a href="/n-com/pro/update/<%= pro._id %>"
                  ><span><ion-icon name="cloud-upload"></ion-icon></span
                  >update</a
                >
              </div>
            </div>
            <!-- singel pro end -->
            <% }) %>
          </div>
        </div>